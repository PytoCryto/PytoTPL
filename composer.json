{
    "name": "pytocryto/pytotpl",
    "description": "A PHP template engine",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "symfony/debug": "~3.2",
        "evenement/evenement": "2.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "PytoTPL\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}